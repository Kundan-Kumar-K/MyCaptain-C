#include<stdio.h>
swap(int *as, int *bs)
{
	int temp;
	temp = *as;
	*as = *bs;
	*bs = temp;
	return 0;
}
main()
{
	int a,b,temp;
	printf("Enter the value for a & b respectively \n ");
	scanf("%d %d",&a,&b);
	printf("The values of a & b are \n a = %d \n b = %d \n",a,b);
	swap(&a,&b);
	printf("After swapping, the values of a & b are\n a = %d \n b = %d \n",a,b);
	return 0;
}
